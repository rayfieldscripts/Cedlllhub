local success, result = pcall(function()
  local remoteCode = game:HttpGet("https://raw.githubusercontent.com/rayfieldscripts/Cedlllhub/refs/heads/main/Spawner")
  if remoteCode then
    local Spawner = loadstring(remoteCode)()
    return Spawner
  else
    warn("Failed to retrieve Spawner script from the URL.")
    return nil
  end
end)

if success then
  local Spawner = result
  -- Continue with your UI code using the Spawner object here...
  print("Spawner loaded successfully!")
else
  error("Error loading Spawner script: " .. result)
end
